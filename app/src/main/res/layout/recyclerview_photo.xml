<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="photoUrlVM"
            type="org.unimelb.itime.ui.viewmodel.event.EventCreateViewModel.PhotoUrlViewModel" />
    </data>

    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:padding="8dp"
        android:onClick="@{photoUrlVM.onClickPhoto()}"
        app:smallPhoto="@{photoUrlVM.photoUrl}" />

</layout>